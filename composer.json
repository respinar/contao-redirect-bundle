{
    "name": "respinar/contao-redirection-bundle",
    "description": " A simple Contao extension for URL redirects (301, 302) and 410 (Gone) responses. ",
    "keywords": ["contao", "bundle", "redirect", "301", "302", "410"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamid Peywasti",
            "email": "hamid@respinar.com",
            "homepage": "https://respinar.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/respinar/contao-redirection-bundle/issues",
        "source": "https://github.com/respinar/contao-redirection-bundle"
    },
    "require": {
        "php": "^8.0",
        "contao/core-bundle": "^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Respinar\\ContaoRedirectionBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Respinar\\ContaoRedirectionBundle\\ContaoManager\\Plugin"
    }
}